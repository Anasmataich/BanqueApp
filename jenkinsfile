pipeline {    
    stages {
        stage('Checkout') {
            
            steps {
                git(
                    url: 'https://github.com/Anasmataich/BanqueApp.git',
                    branch: 'feature-virement',
                )
                
            }
        }
        stage('run the app') {
            steps {
                bat """
                python main.py
                """
            }
        }
    }
 }